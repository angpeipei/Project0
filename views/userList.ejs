<div class="list-view">
    <table class="grid-view">
        <tr>
            <th>Name</th>
            <th>Email address</th>
            <th>Admin</th>
            <th>Action</th>
        </tr>
        <% for(var i=0; i<data.length; i++){ %>
            <tr>
                <td>
                    <%= data[i].name %>
                </td>
                <td>
                    <%= data[i].email %>
                </td>
                <td>
                    <% if (data[i].isAdmin) {%>
                        Yes
                    <%}%>
                </td>                
                <td>&nbsp;</td>
            </tr>
            <% } %>
    </table>
</div>